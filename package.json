{
  "name": "eslint-config-unicorn",
  "description": "An eslint configuration",
  "version": "0.4.0",
  "main": "index.js",
  "license": "MIT",
  "keywords": ["eslint", "eslintconfig"],
  "homepage": "https://github.com/Hilzu/eslint-config-unicorn",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hilzu/eslint-config-unicorn.git"
  },
  "bugs": "https://github.com/Hilzu/eslint-config-unicorn/issues",
  "scripts": {
    "precommit": "lint-staged",
    "test": "eslint index.js"
  },
  "peerDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "1.7.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true
    },
    "rules": {
      "sort-keys": "error"
    }
  },
  "lint-staged": {
    "*.{js,json}": ["prettier --write", "git add"]
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  }
}
